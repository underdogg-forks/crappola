{
    "name": "invoiceninja/invoiceninja",
    "description": "Invoices, expenses & time-tracking built with Laravel",
    "keywords": [
        "invoice",
        "laravel"
    ],
    "license": "Attribution Assurance License",
    "authors": [
        {
            "name": "Hillel Coren",
            "email": "hillelcoren@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "anahkiasen/former": "^4.0",
        "barryvdh/laravel-cors": "^2.0",
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "^2.2",
        "chumper/datatable": "dev-add-back-options",
        "cleverit/ubl_invoice": "^1.0",
        "doctrine/dbal": "^2.9",
        "dompdf/dompdf": "^0.6",
        "ezyang/htmlpurifier": "^v4.12",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "laracasts/presenter": "^0.2",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.7.*",
        "league/fractal": "^0.13",
        "maatwebsite/excel": "^2.0",
        "mpdf/mpdf": "^7.1",
        "nesbot/carbon": "^1.25",
        "nwidart/laravel-modules": "^2.0",
        "patricktalmadge/bootstrapper": "5.5.x",
        "predis/predis": "^1.1",
        "santigarcor/laratrust": "5.2.*",
        "turbo124/laravel-push-notification": "^2.0",
        "webpatser/laravel-countries": "dev-master#75992ad"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "app/Console/Commands",
            "app/Libraries",
            "app/Http/Controllers",
            "app/Models",
            "app/Ninja",
            "app/Ninja/Repositories",
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Libraries/lib_autolink.php",
            "app/Libraries/OFX.php"
        ]
    },
    "autoload-dev": {
        "classmap": []
    },
    "scripts": {
        "post-install-cmd": [
            "rm bootstrap/cache/compiled.php || true",
            "php artisan view:clear",
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "rm bootstrap/cache/compiled.php || true",
            "php artisan view:clear",
            "php artisan clear-compiled"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hillelcoren/datatable"
        }
    ]
}
