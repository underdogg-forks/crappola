{
  "name": "invoiceninja/invoiceninja",
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-zip": "*",
    "anahkiasen/former": "^4.8",
    "awssat/tailwindo": "^3.0",
    "chumper/datatable": "dev-laravel7-support",
    "doctrine/dbal": "^3.6",
    "ezyang/htmlpurifier": "^4.16",
    "fideloper/proxy": "^4.4",
    "guzzlehttp/guzzle": "^7.5",
    "intervention/image": "^2.7",
    "laracasts/presenter": "^0.2",
    "laravel/framework": "^8.83",
    "laravel/helpers": "^1.6",
    "laravel/ui": "^3.4",
    "laravelcollective/html": "^6.4",
    "league/fractal": "^0.20",
    "nesbot/carbon": "^2.66",
    "patricktalmadge/bootstrapper": "^5.12",
    "predis/predis": "^2.1",
    "webpatser/laravel-countries": "^1.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.7",
    "barryvdh/laravel-ide-helper": "^2.13",
    "filp/whoops": "^2.15",
    "fakerphp/faker": "^1.21",
    "laravel/tinker": "^2.8",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^5.11",
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "classmap": [
      "app/Console/Commands",
      "app/Libraries",
      "app/Http/Controllers",
      "app/Models",
      "app/Ninja",
      "app/Ninja/Repositories",
      "database/seeders",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Libraries/lib_autolink.php",
      "app/Libraries/OFX.php"
    ]
  },
  "autoload-dev": {},
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "rm bootstrap/cache/compiled.php || true",
      "php artisan view:clear",
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "rm bootstrap/cache/compiled.php || true",
      "php artisan view:clear",
      "php artisan clear-compiled"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/turbo124/chumper-datatable"
    }
  ]
}
