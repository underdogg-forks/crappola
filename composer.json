{
    "name": "invoiceninja/invoiceninja",
    "description": "Invoices, expenses & time-tracking built with Laravel",
    "keywords": [],
    "license": "",
    "authors": [],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "codeception/c3": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "anahkiasen/former": "*",
        "asgrim/ofxparser": "*",
        "bacon/bacon-qr-code": "*",
        "barracudanetworks/archivestream-php": "*",
        "barryvdh/laravel-cors": "*",
        "cerdic/css-tidy": "*",
        "chumper/datatable": "dev-laravel-8-beyond",
        "cleverit/ubl_invoice": "*",
        "codedge/laravel-selfupdater": "^3.6",
        "doctrine/dbal": "*",
        "dompdf/dompdf": "*",
        "ezyang/htmlpurifier": "*",
        "google/apiclient": "*",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "*",
        "intervention/image": "*",
        "intervention/image-laravel": "*",
        "jlapp/swaggervel": "*",
        "laracasts/presenter": "dev-master",
        "laravel/framework": "^10.0",
        "laravel/helpers": "*",
        "laravel/legacy-encrypter": "*",
        "laravel/socialite": "*",
        "laravel/ui": "*",
        "spatie/laravel-html": "*",
        "league/csv": "*",
        "league/fractal": "*",
        "maatwebsite/excel": "dev-php-fix",
        "mpdf/mpdf": "*",
        "nesbot/carbon": "*",
        "nwidart/laravel-modules": "2.0.*",
        "patricktalmadge/bootstrapper": "dev-l10-compatibility",
        "pragmarx/google2fa-laravel": "*",
        "predis/predis": "*",
        "simshaun/recurr": "dev-master",
        "stripe/stripe-php": "*",
        "symfony/css-selector": "*",
        "symfony/http-client": "*",
        "symfony/mailer": "*",
        "symfony/postmark-mailer": "*",
        "therobfonz/laravel-mandrill-driver": "*",
        "vemcogroup/laravel-sparkpost-driver": "^5.0",
        "webpatser/laravel-countries": "dev-master#75992ad"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": "*",
        "codeception/c3": "*",
        "codeception/codeception": "*",
        "fakerphp/faker": "*",
        "filp/whoops": "*",
        "laravel/pint": "^1.18",
        "laravel/tinker": "*",
        "nunomaduro/collision": "^6.1",
        "phpspec/phpspec": "*",
        "phpunit/phpunit": "*",
        "rector/rector": "0.14.8",
        "spatie/laravel-ignition": "*",
        "symfony/dom-crawler": "*"
    },
    "autoload": {
        "classmap": [
            "app/Console/Commands",
            "app/Libraries",
            "app/Http/Controllers",
            "app/Models",
            "app/Ninja",
            "app/Ninja/Repositories",
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories",
            "Database\\Seeders\\": "database/seeders",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/helpers.php",
            "app/Libraries/lib_autolink.php",
            "app/Libraries/OFX.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "rm bootstrap/cache/compiled.php || true",
            "php artisan view:clear",
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "rm bootstrap/cache/compiled.php || true",
            "php artisan view:clear",
            "php artisan clear-compiled"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "replace": {
        "alfaproject/omnipay-skrill": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-google-cloud-storage.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Applicomhq/bootstrapper.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hillelcoren/l5-google-cloud-storage"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hillelcoren/omnipay-wepay"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hillelcoren/omnipay-gocardlessv2"
        },
        {
            "type": "vcs",
            "url": "https://github.com/underdogg-forks/chumper-datatable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/turbo124/php-phantomjs"
        },
        {
            "type": "vcs",
            "url": "https://github.com/davidbankes/omnipay-firstdata.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hillelcoren/omnipay-authorizenet"
        },
        {
            "type": "vcs",
            "url": "https://github.com/turbo124/omnipay-common"
        },
        {
            "type": "vcs",
            "url": "https://github.com/turbo124/laravel-push-notification"
        },
        {
            "type": "vcs",
            "url": "https://github.com/turbo124/NotificationPusher"
        },
        {
            "type": "vcs",
            "url": "https://github.com/turbo124/Laravel-Excel"
        }
    ]
}
