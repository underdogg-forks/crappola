        "nwidart/laravel-modules": "2.0.*",
        "omnipay/authorizenet": "dev-solution-id as 2.5.0",
        "omnipay/bitpay": "dev-master",
        "omnipay/braintree": "^1.1",
        "omnipay/common": "2.5.2",
        "omnipay/firstdata": "^2.4",
        "omnipay/gocardless": "dev-master",
        "omnipay/mollie": "3.*",
